{
  "name": "@sunne-test/back",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev:back": "tsx watch src/server.ts",
    "lint": "biome check --fix --unsafe ./src/**",
    "lint:ci": "biome check --diagnostic-level=error ./src/**",
    "migrate:ts": "tsx watch ./src/migrate.ts",
    "migrate:js": "node ./dist/migrate.js",
    "seeds:ts": "tsx watch ./src/seeders.ts",
    "seeds:js": "node ./dist/seeders.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.ts",
    "test:e2e:cov": "vitest run --config ./vitest.config.e2e.ts --coverage",
    "test:e2e:watch": "vitest --config ./vitest.config.e2e.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "catalog:",
    "@faker-js/faker": "^9.4.0",
    "@types/axios": "^0.14.4",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "catalog:",
    "@types/sequelize": "^4.28.20",
    "@types/supertest": "^6.0.2",
    "@vitest/coverage-v8": "2.1.5",
    "sqlite3": "^5.1.7",
    "typescript": "catalog:",
    "vite-tsconfig-paths": "catalog:",
    "vitest": "catalog:"
  },
  "dependencies": {
    "@fastify/cors": "^10.0.1",
    "@fastify/jwt": "^9.0.3",
    "@fastify/swagger": "^9.3.0",
    "@fastify/swagger-ui": "^5.1.0",
    "@node-rs/argon2": "^2.0.0",
    "axios": "^1.7.9",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.7",
    "fastify": "^5.1.0",
    "fastify-plugin": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "nanoid": "^5.0.8",
    "nock": "^14.0.0",
    "pg": "^8.13.1",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.5",
    "sequelize-typescript": "^2.1.6",
    "supertest": "^7.0.0",
    "tsx": "^4.19.2",
    "umzug": "^3.8.2",
    "unplugin-swc": "^1.5.1",
    "uuid": "^11.0.5",
    "zod": "^3.24.1"
  }
}
